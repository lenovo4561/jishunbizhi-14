{
  "package": "com.jishunclbz.jsauspicious",
  "name": "吉顺壁纸",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.app"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.media"
    },
    {
      "name": "system.request"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "off",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Splash",
    "pages": {
      "pages/Splash": {
        "component": "index"
      },
      "pages/Web": {
        "component": "index"
      },
      "widgets/pages/Main": {
        "component": "index"
      },
      "widgets/pages/Demo": {
        "component": "index"
      },
      "widgets/pages/DemoDetail": {
        "component": "index"
      },
      "widgets/pages/Webview": {
        "component": "index"
      },
      "widgets/pages/CategoryList": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "widgets",
      "resource": "widgets"
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Splash": {
        "titleBar": false,
        "menu": false
      },
      "pages/Web": {
        "titleBarText": "协议"
      },
      "widgets/pages/Main": {
        "titleBar": false,
        "menu": false
      },
      "widgets/pages/Demo": {
        "titleBarText": "快应用示例模版",
        "menu": false
      },
      "widgets/pages/DemoDetail": {
        "titleBarText": "详情页"
      },
      "widgets/pages/Webview": {
        "titleBar": true,
        "menu": false
      },
      "widgets/pages/CategoryList": {
        "titleBar": false,
        "menu": false
      }
    }
  },
  "deviceTypeList": ["phone"]
}
